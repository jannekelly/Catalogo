<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jsfhibernate.pojo.Televisores" table="televisor" catalog="jsfhibercrud" optimistic-lock="version">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="tipo" type="string">
            <column name="Tipo" />
        </property>
        <property name="so" type="string">
            <column name="So" />
        </property>
    </class>
</hibernate-mapping>